<html>
<head>
<title><%= page_title %></title>
<%= javascript_include_tag :defaults -%>
<%= stylesheet_link_tag :all %>
</head>
<body>
	
<%= render :partial => "layouts/header" -%>	

<div id="content_wrap">
	<div id="content">
	
		<% if flash[:notice] %>
			<div class='flash'><%= flash[:notice] %></div>
		<% end %>
	
		<%= yield -%>
	</div>
</div>

<%= render :partial => "layouts/footer" -%>
</body>
</html>